{
    "multipart": [
        {   "when": { "up": "true" },
            "apply": [
				{ "model": "block/cobblestone_wall_post" },
				{ "model": "block/cobblestone_wall_post1" }
			]
        },
        {   "when": { "north": "true" },
            "apply": [
				{ "model": "block/cobblestone_wall_side", "uvlock": true },
				{ "model": "block/cobblestone_wall_side1", "uvlock": true }
			]
        },
        {   "when": { "east": "true" },
            "apply": [
				{ "model": "block/cobblestone_wall_side", "y": 90, "uvlock": true },
				{ "model": "block/cobblestone_wall_side1", "y": 90, "uvlock": true }
			]
        },
        {   "when": { "south": "true" },
            "apply": [
				{ "model": "block/cobblestone_wall_side", "y": 180, "uvlock": true },
				{ "model": "block/cobblestone_wall_side1", "y": 180, "uvlock": true }
			]
        },
        {   "when": { "west": "true" },
            "apply": [
				{ "model": "block/cobblestone_wall_side", "y": 270, "uvlock": true },
				{ "model": "block/cobblestone_wall_side1", "y": 270, "uvlock": true }
			]
        }
    ]
}